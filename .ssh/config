Include ~/.orbstack/ssh/config

Host nuc
  HostName nuc.lan
  User kid

Host mac
  HostName mac.lan
  User kid

Host r2s
  HostName r2s.lan
  User root

Host nuc.nanova.me
  HostName nuc.nanova.me
  ProxyCommand /usr/local/bin/cloudflared access ssh --hostname %h

Host r2s.nanova.me
  HostName r2s.nanova.me
  User root
  ProxyCommand /usr/local/bin/cloudflared access ssh --hostname %h

