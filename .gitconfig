[core]
  pager = delta

[interactive]
  diffFilter = delta --color-only

[color]
  ui = auto

[delta]
  navigate = true

[merge]
  conflictstyle = diff3

[diff]
  colorMoved = default

[push]
  autoSetupRemote = true

[credential]
	helper = /usr/local/bin/git-credential-manager
	credentialStore = cache

[credential "https://dev.azure.com"]
	useHttpPath = true
